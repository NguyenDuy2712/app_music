<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <de.hdodenhof.circleimageview.CircleImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/imv1"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="156dp"
        android:layout_height="156dp"
        android:src="@drawable/denvau"
        app:civ_border_width="2dp"
        app:civ_border_color="@color/white"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/imv3"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="156dp"
        android:layout_height="156dp"
        android:src="@drawable/binz"
        app:civ_border_width="2dp"
        app:civ_border_color="@color/white"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/imv2"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="196dp"
        android:layout_height="186dp"
        android:src="@drawable/sontung"
        app:civ_border_width="4dp"
        app:civ_border_color="@color/white"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/lb1"
        android:text="T.A.P"
        app:layout_constraintTop_toBottomOf="@+id/imv1"
        app:layout_constraintStart_toStartOf="@+id/imv1"
        app:layout_constraintEnd_toEndOf="@+id/imv1"
        android:layout_marginTop="5dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:layout_marginStart="5dp"
        android:gravity="center"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="T.A.P"
        android:id="@+id/lb2"
        app:layout_constraintTop_toBottomOf="@+id/imv2"
        app:layout_constraintStart_toStartOf="@+id/imv2"
        app:layout_constraintEnd_toEndOf="@+id/imv2"
        android:layout_marginTop="5dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:layout_marginStart="5dp"
        android:gravity="center"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="T.A.P"
        android:id="@+id/lb3"
        app:layout_constraintTop_toBottomOf="@+id/imv3"
        app:layout_constraintStart_toStartOf="@+id/imv3"
        app:layout_constraintEnd_toEndOf="@+id/imv3"
        android:layout_marginTop="5dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:layout_marginStart="5dp"
        android:gravity="center"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</androidx.constraintlayout.widget.ConstraintLayout>